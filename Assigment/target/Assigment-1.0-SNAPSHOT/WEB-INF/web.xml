<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>ListFood</servlet-name>
        <servlet-class>com.example.assigment1.assigment.controller.ListFoodServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListFood</servlet-name>
        <url-pattern>/admin/list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateFood</servlet-name>
        <servlet-class>com.example.assigment1.assigment.controller.CreateNewFood</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateFood</servlet-name>
        <url-pattern>/admin/create</url-pattern>
    </servlet-mapping>

    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <page-encoding>UTF-8</page-encoding>
        </jsp-property-group>
    </jsp-config>
</web-app>